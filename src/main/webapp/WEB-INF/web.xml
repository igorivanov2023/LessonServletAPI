<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>TimeMinsk</servlet-name>
    <servlet-class>com.tms.TimeMinsk</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TimeWashington</servlet-name>
    <servlet-class>com.tms.TimeWashington</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TimeВeijing</servlet-name>
    <servlet-class>com.tms.TimeBeijing</servlet-class>
  </servlet>

   <servlet-mapping>
    <servlet-name>TimeMinsk</servlet-name>
    <url-pattern>/minsk</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TimeWashington</servlet-name>
    <url-pattern>/washington</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TimeВeijing</servlet-name>
    <url-pattern>/beijing</url-pattern>
  </servlet-mapping>

</web-app>


